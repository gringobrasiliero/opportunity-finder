<%= form_for(@profile), method: "put" do |f| %>

<%= f.label :first_name %>
<%= f.text_field :first_name, @profile.first_name  %>

<%= f.label :last_name %>
<%= f.text_field :last_name, @profile.last_name %>

<%= f.label :location %>
<%= f.text_field :location, @profile.location %>

<%= f.label :profession %>
<%= f.text_field :profession, @profile.profession %>

<%= f.submit %>

<% end %>
